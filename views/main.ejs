<!DOCTYPE HTML>
<html>
  <head>
    <title>Block Chain</title>
    <link rel="stylesheet" href="css/main.css" />
    <meta name="viewport" content="width=device-width, user-scalable=no
      initial-scale=1.0, minimum-scale=1.0, maximum-scale=1.0" />
  </head>
  <body>
    <header>
      <input type="checkbox" id="menu-bar" />
      <label for="menu-bar"><span></span></label>
      <nav>
        <div class="user_info">
          <div class="img-wrap">
            <img src="img/profile_face2.jpg" />
          </div>
          <div class="profile">
            <p class="username">Alexander Great</p>
            <p class="user_detail">Administrator</p>
            <img src="img/icon-setting.svg" class="icon-setting" />
          </div>
        </div>
        <div class="menu-container">

          <a href="./devices/">Devices</a>
          
          <a href="#">Reservation</a>
          <a href="#">Users</a>
          <a href="#">History</a>
        </div>
      </nav>
      <label for="menu-bar" class="overlay"></label>
    </header>

    <div class="content">
      <div class="object-icon-wrap">
        <img src="img/icon-airconditioner2.svg" class="object-icon" />
      </div>
      <p class="object-title">
        Air Conditioner
      </p>
      <div class="object-switch">
        <input type="checkbox" id="aircon-switch" />
        <label for="aircon-switch"><span></span></label>
      </div>

    </div>

  </body>
</html>
